#include <stdio.h>

#include <stdlib.h>

int main(){

    int x, i;

    printf("\n Enter the number to find its divisor: ");

    scanf("%d", &x);

    printf("All the divisors of the given number are: ");

    for(i=1; i <= x; i++){

        if(x%i==0){

            printf("\n%d", i);

            printf("\n");

        }

    }

    return 0;

}
